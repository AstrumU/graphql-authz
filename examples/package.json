  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc --build .",
    "start:apollo-server-schema-first": "node dist/apollo-server-schema-first/index.js",
    "start:apollo-server-code-first": "node dist/apollo-server-code-first/index.js",
    "start:nestjs": "node dist/nestjs/index.js",
    "start:type-graphql": "node dist/type-graphql/index.js",
    "start:schema-stitching": "node dist/schema-stitching/index.js & node dist/schema-stitching/services/users/index.js & node dist/schema-stitching/services/posts/index.js",
    "start:apollo-federation": "node dist/apollo-federation/index.js & node dist/apollo-federation/services/users/index.js & node dist/apollo-federation/services/posts/index.js"
