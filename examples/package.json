    "apollo-server-plugin-base": "0.9.1"
  },
  "devDependencies": {
    "@apollo/federation": "0.28.0",
    "@apollo/gateway": "0.37.0",
    "@graphql-tools/stitch": "7.5.3",
    "@graphql-tools/stitching-directives": "1.3.3",
    "@nestjs/common": "7.6.5",
    "@nestjs/core": "7.6.4",
    "@nestjs/graphql": "7.9.8",
    "@nestjs/platform-express": "7.6.4",
    "@types/lodash": "4.14.165",
    "@types/jest": "26.0.20",
    "@types/wait-on": "5.3.1",
    "apollo-server": "2.22.2",
    "apollo-server-core": "2.21.2",
    "cross-fetch": "3.1.4",
    "jest": "^26.6.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.3",
    "ts-node": "^9.1.1",
    "type-graphql": "1.1.1",
    "typescript": "^4.1.3",
    "wait-on": "6.0.0"
